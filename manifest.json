{
  "name": "Matrix Highlight",
  "description": "Matrix-based page highlighting",
  "version": "0.1.0",
  "manifest_version": 2,
  "browser_specific_settings": {
	"gecko": {
	  "id": "matrix-highlight@danilafe",
	  "strict_min_version": "67.0"
	}
  },
  "background": {
	"scripts": [
	  "background.js"
	]
  },
  "browser_action": {},
  "commands": {
	"_execute_browser_action": {
	  "suggested_key": {
		"default": "Alt+Shift+H"
	  }
	}
  },
  "permissions": [
	"storage",
	"contextMenus",
	"activeTab",
	"tabs"
  ]
}
